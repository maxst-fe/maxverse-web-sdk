{
  "name": "@maxverse/passport-react",
  "version": "0.4.5",
  "keywords": [
    "login",
    "Authorization Code Grant Flow",
    "PKCE",
    "SPA",
    "React"
  ],
  "homepage": "https://doc.maxverse.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxverse-dev/maxverse-web-sdk"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./esm/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./esm/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "rm -rf dist esm && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "prepack": "yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.16.3",
    "@maxverse/passport-web-sdk": "^0.5.4",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.1",
    "@types/node": "^14.14.35",
    "@types/react": "^18.0.21",
    "@yarnpkg/pnpify": "^4.0.0-rc.44",
    "babel-jest": "^29",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "ts-jest": "^29.1.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.5.1",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./src/index.ts",
    "module": "./src/index.ts"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18"
  }
}
